{
  "manifest_version": 3,
  "name": "Nazwa twojej wtyczki",
  "version": "1.0",
  "description": "Opis twojej wtyczki",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}